#include "../include/Configuration.h"
#include <iostream>
using std::endl;
using std::cout;
using namespace wd;
int main()
{

    Configuration *conf =  Configuration::getInstance("../conf/my.bat");
    
    cout << "my_ip: " << conf->ip() << endl;
    cout << "my_port: " << conf->port() << endl;
    cout << "my_dict: " << conf->enDcitPpath() << endl;
    cout << "my_index: " << conf->enIndexPath() << endl;
    cout << "my_zn_dict: " << conf->znDictPath() << endl;
    cout << "my_zn_index: " << conf->znIndexPath() << endl;
    cout << "my_cache: " << conf->cachePath() << endl;
    cout << "my_thread_num: " << conf->threadNum() << endl;
    cout << "my_buf_size: " << conf->bufSize() << endl;
    cout << "my_init_size: " << conf->initSize() << endl;
    cout << "my_init_time: " << conf->initTime() << endl;
    cout << "my_update_time: " << conf->updateTime() << endl;
    return 0;
}

